{
    "version": "0.1.0",
    "_runner": "terminal",
    "echoCommand": true,
    "tasks": [
        {
            "taskName": "build",
            "command": "dotnet build",
            "isShellCommand": true,
            "showOutput": "always",
            "isBuildCommand": true,
            "problemMatcher": "$msCompile"
        },
        {
            "taskName": "test",
            "command": "dotnet test",
            "isTestCommand": true
        },
        {
            "taskName": "antlr",
            "args": [
                "pushd ${workspaceRoot}\\DiceShow.Ops\\Parsing & antlr Dice.g4 -package DiceShow.Ops.Parsing.Internal & echo DONE"
            ],
            "showOutput": "always"
        }
    ]
}